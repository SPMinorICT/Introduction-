c = True
while True:
    n = input()
    if n == 'stop':
        break
    if int(n) % 2 == 1:
        print("Weird")

    elif int(n) % 2 == 0 and range(2, 5):
        print('Not weird')

    elif int(n) % 2 == 0 and range(6, 20):
        print("Weird")

    elif int(n) % 2 == 0 and int(n) > 20:
        print('Not weird')
